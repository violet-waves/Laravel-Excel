{
  "name": "violet-waves/excel",
  "description": "Supercharged Excel exports and imports in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "phpspreadsheet",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@spartner.nl"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": "^8.0",
    "phpoffice/phpspreadsheet": "^1.29.4||^2.0",
    "illuminate/support": "^9.0||^10.0||^11.0",
    "psr/simple-cache": "^1.0||^2.0||^3.0",
    "composer/semver": "^3.3"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0||^8.0||^9.0||^10.0",
    "predis/predis": "^1.1",
    "laravel/scout": "^9.0||^10.0||^11.0"
  },
  "autoload": {
    "psr-4": {
      "VioletWaves\\Excel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VioletWaves\\Excel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "VioletWaves\\Excel\\ExcelServiceProvider"
      ],
      "aliases": {
        "Excel": "VioletWaves\\Excel\\Facades\\Excel"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
